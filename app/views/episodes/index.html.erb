<h1>All Episodes</h1>
<p> <%= link_to("Go to Guests", guests_path) %>  || <%= link_to("Create new Appearance", new_appearance_path) %></p>
<ul>
  <% @episodes.each do |episode| %>
    <li>
    	<%= link_to("Episode #{episode.number} - #{episode.date.strftime("%B %d, %Y")}", episode_path(episode)) %>
    </li>
  <% end %>
</ul>
