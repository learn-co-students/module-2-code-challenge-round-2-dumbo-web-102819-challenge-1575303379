<h1>Edit Guest Info</h1>

<%= form_for @guest do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :occupation %>
    <%= f.text_field :occupation %>
    <br>
    <%= f.submit %>
<% end %>

<br>
<%= button_to "Back to Guests", guests_path, :method => "get" %>
<%= button_to "Back to the Main Page", home_path, :method => "get" %>